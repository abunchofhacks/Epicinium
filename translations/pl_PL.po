#: src/ai/aichallenge.cpp:713
msgid "Greetings, Commander!"
msgstr "Witaj, Dowódco!"

#: src/ai/aichallenge.cpp:480
msgid "Defeat the enemy."
msgstr "Pokonaj wroga."

#: src/ai/aichallenge.cpp:485 src/ai/aichallenge.cpp:597
#: src/ai/aichallenge.cpp:672 src/ai/aichallenge.cpp:703
#: src/ai/aichallenge.cpp:741 src/ai/aichallenge.cpp:774
